{
  "name": "stock-valuation-mcp-server",
  "version": "1.0.0",
  "description": "MCP Server for Stock Valuation Calculations (PE Band, DDM, DCF)",
  "main": "dist/index.js",
  "type": "module",
  "scripts": {
    "build": "tsc",
    "start": "node dist/index.js",
    "start:stdio": "node dist/index.js --stdio",
    "dev": "tsx src/index.ts",
    "dev:stdio": "tsx src/index.ts --stdio",
    "test": "jest",
    "clean": "rimraf dist",
    "agent": "node agents/cli.mjs",
    "agent:dev": "node agents/cli.mjs dev",
    "agent:qa": "node agents/cli.mjs qa",
    "agent:workflow": "node agents/cli.mjs workflow",
    "agent:status": "node agents/cli.mjs status",
    "agent:convert": "node agents/cli.mjs convert-to-v2",
    "agent:validate": "node agents/cli.mjs validate",
    "agent:test": "node agents/test-agent.mjs"
  },
  "keywords": [
    "mcp",
    "stock",
    "valuation",
    "finance",
    "pe-band",
    "ddm",
    "dcf"
  ],
  "author": "Your Name",
  "license": "MIT",
  "dependencies": {
    "@modelcontextprotocol/sdk": "^1.24.3",
    "@types/cheerio": "^1.0.0",
    "axios": "^1.6.2",
    "cheerio": "^1.1.2",
    "dotenv": "^16.3.1",
    "express": "^4.18.2"
  },
  "devDependencies": {
    "@types/express": "^5.0.6",
    "@types/jest": "^29.5.11",
    "@types/node": "^20.10.5",
    "jest": "^29.7.0",
    "rimraf": "^5.0.5",
    "ts-jest": "^29.1.1",
    "tsx": "^4.7.0",
    "typescript": "^5.3.3"
  },
  "engines": {
    "node": ">=20.18.1"
  }
}
